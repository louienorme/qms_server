    "morgan": "^1.10.0",
    "twilio": "^3.76.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/luxon": "^2.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.10",
    "eslint": "^8.1.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
