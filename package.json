  "bugs": {
    "url": "https://github.com/FreshMaker-L/qms_server/issues"
  },
  "homepage": "https://github.com/FreshMaker-L/qms_server#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-slow-down": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.3.0",
    "express-slow-down": "^1.4.0",
    "faker": "^5.5.3",
    "http": "^0.0.1-security",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
