{
  "name": "qms_server",
  "version": "1.0.0",
  "description": "qms-server built in MERN",
  "main": "server.ts",
  "scripts": {
    "start": "node ./build/src/server.js",
    "start:dev": "nodemon ./src/server.ts" ,
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./build/src/server.js",
    "build": "tsc",
    "seed": "ts-node -r tsconfig-paths/register bin/seed.ts",
    "seed:safe": "ts-node -r tsconfig-paths/register bin/seed:safe.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FreshMaker-L/qms_server.git"
  },
